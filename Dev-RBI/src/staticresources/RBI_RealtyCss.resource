body input.btn,
.btn{
	 margin: 3px;
	font-family:Helvetica, Arial, sans-serif;
	opacity:1;
	cursor:pointer;
	border:none;
	color: #FFFFFF;
	padding: 3px 20px 5px;
	text-align: center;
	position:relative;
	-webkit-border-radius: 6px;
	display:inline-block;
	-moz-border-radius: 6px;
	border-radius: 6px;
	-webkit-box-shadow: #666 0px 2px 3px;
	-moz-box-shadow: #666 0px 2px 3px;
	box-shadow: #666 0px 2px 3px;
	background: #05c0ff;
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#05c0ff), color-stop(100%,#048cda));
	background: -webkit-linear-gradient(top, #05c0ff 0%,#048cda 100%);
	background: -moz-linear-gradient(top, #05c0ff 0%, #048cda 100%);
	background: -ms-linear-gradient(top, #05c0ff 0%,#048cda 100%);
	background: -o-linear-gradient(top, #05c0ff 0%,#048cda 100%);
	background: linear-gradient(top, #05c0ff 0%,#048cda 100%);
	-pie-background: linear-gradient(top, #05c0ff 0%,#048cda 100%);
    behavior: url('/resources/PIE.htc');
    /*IE7+8 ClearType "jagged font" fix */
	/* 	filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src=hIEfix.png,sizingMethod=crop); */
	zoom:1;
}
