    <validationRules>
        <fullName>Floor_Validation</fullName>
        <active>true</active>
        <description>User will not allowed to enter not more that total floors in tower (In Parent Total Floors formula field)object.</description>
        <errorConditionFormula>IF(ISBLANK(Total_Floors__c) || (Total_Floors__c==0), false, (VALUE(TEXT(Floor__c)) &gt; Total_Floors__c))</errorConditionFormula>
        <errorMessage>Your floor value is exceeding the Total number of floor for this particular tower.</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Add_Units</fullName>
        <availability>online</availability>
        <description>This button will allow the user to create multiple units for a Tower</description>
        <displayType>massActionButton</displayType>
        <linkType>url</linkType>
        <masterLabel>Add Units</masterLabel>
        <openType>replace</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>\apex\RBI_CreateUnits?ProjId={!Tower__c.ProjectId__c}&amp;TowName={!Tower__c.Name}&amp;TowId={!Tower__c.Id}&amp;unittotal={!Tower__c.Total_Units__c}</url>
    </webLinks>
    <webLinks>
        <fullName>Mass_Edit_Units</fullName>
